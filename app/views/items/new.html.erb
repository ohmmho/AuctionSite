<h3>New Auction</h3>
<p>New Item to Bid</p>
<%= form_for @item do |f| %>

  

      <%= f.label :name %>
      <%= f.text_field :name, placeholder: "Item name" %>

      <%= f.label "End of the auction date" %>
      <%= f.date_field :end_date %>
  
    <%= f.submit 'Publish Auction' %>
  
<% end %>